.SUFFIXES : .x .o .c .s

BUILD_ROOT=/usr/local/arm_tools/
lib=${BUILD_ROOT}arm-elf/lib
lib1=${BUILD_ROOT}lib/gcc-lib/arm-elf/3.0/
include=${BUILD_ROOT}arm-elf/inc

CC=arm-elf-gcc -I${include} -I. -I.. -I../include -O1 -DEMBED -DOPENSSL_NO_KRB5 -DOPENSSL_NO_ENGINE -DOPENSSL_NO_HW -DOPENSSL_NO_ASM -DL_ENDIAN -DTERMIO -fomit-frame-pointer -Wall -c

AS=arm-elf-as
LD=arm-elf-ld

