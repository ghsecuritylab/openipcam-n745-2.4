#LIBS= -lhttp -lwbrc -lpthread -lgcc -lwbrc
LIBS= -lhttp -lc -lgcc -lc

TARGET = crypto

all: lib

lib: $(OBJS)
	rm -rf lib$(TARGET).a
	$(AR) -r lib$(TARGET).a $(OBJS)

.c.o:
	$(CC) -o $*.o $*.c

.s.o:
	$(AS) -ahld -o $*.o $*.s > NULL
	del NULL


clean:
	rm -f *.o
	rm -f *.x
	rm -f *.map
	rm -f romfs.img
	rm -f ${OBJS}
